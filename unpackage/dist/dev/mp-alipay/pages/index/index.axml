<view class="content"><view class="navbar"><block a:for="{{navList}}" a:for-item="item" a:for-index="index" a:key="index"><view data-event-opts="{{[['tap',[['tabClick',[index]]]]]}}" class="{{(('nav-item')+' '+((tabCurrentIndex===index)?'current':''))}}" onTap="__e">{{''+item.text+''}}</view></block></view><swiper class="swiper-box" current="{{tabCurrentIndex}}" duration="300" data-event-opts="{{[['change',[['changeTab',['$event']]]]]}}" onChange="__e"><block a:for="{{navList}}" a:for-item="tabItem" a:for-index="tabIndex" a:key="tabIndex"><swiper-item class="tab-content"><scroll-view class="list-scroll-content" scroll-y data-event-opts="{{[['scrollToLower',[['loadData',['$event']]]]]}}" onScrollToLower="__e"><block a:if="{{tabItem.loaded===true&&tabItem.videoList.length===0}}"><empty vue-id="{{'1-'+tabIndex}}" onVueInit="__l"></empty></block><block a:for="{{tabItem.videoList}}" a:for-item="item" a:for-index="index" a:key="index"><view class="video-item"><view class="user-container"><image class="user-icon" src="{{item.userface}}" mode="aspectFill"></image><view class="user-name"><text>{{item.username}}</text></view></view><view class="desc-container"><text>{{item.desc}}</text></view><view class="picture-container"><image class="picture" src="{{item.image}}" mode="aspectFill" data-event-opts="{{[['tap',[['navToVideoPage',[['o',['desc','电商产品预计下周上架为双11奋斗']]]]]]]}}" onTap="__e"></image></view></view></block><uni-load-more vue-id="{{'2-'+tabIndex}}" status="{{tabItem.loadingType}}" onVueInit="__l"></uni-load-more></scroll-view></swiper-item></block></swiper></view>